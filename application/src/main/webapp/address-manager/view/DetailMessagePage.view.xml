<mvc:View
	controllerName="sap.ui.demo.addressmgr.controller.DetailMessagePage"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m">
	<MessagePage
		id="page"
		title="{i18n>detailTitle}"
		text="{
			path: 'detailMessageView>/messageType',
			formatter: '.messageText'
		}"
		icon="{sap-icon://account}">
	</MessagePage>
</mvc:View>
